{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "aaaa",
  "id": "call-activity-template",
  "version": 1,
  "description": "aaaaa",
  "appliesTo": [
    "bpmn:Activity"
  ],
  "elementType": {
    "value": "bpmn:CallActivity"
  },
  "category": {
    "id": "templates",
    "name": "Templates"
  },
  "groups": [
    {
      "id": "templated-properties",
      "label": "Templated properties",
      "openByDefault": true
    },
    {
      "id": "io-mapping",
      "label": "I/O Mapping Example",
      "openByDefault": true
    }
  ],
  "properties": [
    {
      "type": "String",
      "value": "<insert-the-process-id>",
      "binding": {
        "type": "zeebe:calledElement",
        "property": "processId"
      },
      "label": "Called Process ID",
      "description": "ID of the process to be called by this call activity",
      "group": "templated-properties"
    },
    {
      "type": "Hidden",
      "value": "versionTag",
      "binding": {
        "type": "zeebe:calledElement",
        "property": "bindingType"
      },
      "label": "Binding",
      "description": "Binding type for the external resource"
    },
    {
      "type": "Hidden",
      "value": "<insert-the-version-tag>",
      "binding": {
        "type": "zeebe:calledElement",
        "property": "versionTag"
      },
      "label": "Version Tag",
      "description": "Version tag by which the called process will be referenced."
    },
    {
      "label": "Source variable for the input",
      "type": "String",
      "value": "=insert-the-input",
      "binding": {
        "type": "zeebe:input",
        "name": "myInput"
      },
      "feel": "required",
      "group": "io-mapping"
    },
    {
      "label": "Target variable for the output",
      "type": "String",
      "value": "=insert-the-output",
      "binding": {
        "type": "zeebe:output",
        "source": "=insert-the-output"
      },
      "feel": "required",
      "group": "io-mapping"
    }
  ]
}